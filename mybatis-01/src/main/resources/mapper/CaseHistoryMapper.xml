<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.yi.mapper.CaseHistoryMapper">

    <update id="updateCaseHistoryByCaseId" parameterType="CaseHistory">
 UPDATE  casehistory SET sickinfo=#{sickinfo}
 WHERE caseid=#{caseid} ;
    </update>
</mapper>